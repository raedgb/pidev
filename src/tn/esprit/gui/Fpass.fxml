<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="646.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.gui.FpassController">
   <children>
      <TextField fx:id="tfemail_tel" layoutX="208.0" layoutY="148.0" prefHeight="42.0" prefWidth="230.0" promptText="Email" />
      <Button fx:id="btsearch" layoutX="241.0" layoutY="217.0" mnemonicParsing="false" onAction="#search" prefHeight="42.0" prefWidth="165.0" style="-fx-background-color: #4286f4; -fx-background-radius: 30px;" text="Search for email" textFill="WHITE">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="tfverificationcode" layoutX="208.0" layoutY="229.0" prefHeight="42.0" prefWidth="230.0" promptText="Verification code" />
      <PasswordField fx:id="pfnew_password" layoutX="209.0" layoutY="296.0" prefHeight="42.0" prefWidth="230.0" promptText="new password" />
      <PasswordField fx:id="pfconfirm" layoutX="209.0" layoutY="361.0" prefHeight="42.0" prefWidth="230.0" promptText="confirm password" />
      <Button fx:id="btupdate" layoutX="236.0" layoutY="444.0" mnemonicParsing="false" onAction="#update" prefHeight="42.0" prefWidth="177.0" style="-fx-background-color: #4286f4; -fx-background-radius: 30px;" text="Update" textFill="WHITE">
         <font>
            <Font name="Times New Roman Bold" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
